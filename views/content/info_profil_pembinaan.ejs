                        <div class="card-body">
                          <div class="row">
                              <div class="col-md-12 col-lg-12">
                                  <table class="table table-bordered">
                                    <thead>
                                        <tr>
                                            <th>No.</th>
                                            <th>Nama / Jabatan Dalam Kedinasan</th>
                                            <th>Nama</th>
                                            <th>Jabatan</th>
                                        </tr>
                                    </thead>

                                    <tbody id="tpjk">
                                        
                                    </tbody>
                                  </table>
                              </div>
                          </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
