<body>
    
    <% include ../template/header.ejs %>


    <section style="padding: 90px 0;">
        <div class="container">
            <div class="row">
                <div class="col-md-12 col-lg-12">
                    <h2 class="m-t-0 m-b-0 text-center"><strong>DATA TENAGA KERJA PROVINSI JAWA TENGAH</strong></h2>
                </div>
            </div>

            <div class="row" style="margin-top: 30px;">
                <div class="col-md-8 col-lg-8">
                    <div id="chartContainer" style="height: 800px; width: 100%;"></div>
                </div>

                <div class="col-md-4 col-lg-4">
                    <table class="table table-bordered datane">
                        <thead>
                            <tr>
                                <th rowspan="2">Kabupaten/Kota</th>
                                <th colspan="2">Jumlah Tenaga Kerja</th>
                            </tr>

                            <tr>
                                <th>SKA</th>
                                <th>SKT</th>
                            </tr>
                        </thead>

                        <tbody>
                            <% for(var i=0; i<data_tenaga_kerja.length; i++){ %>
                            <tr>
                                <td><%= data_tenaga_kerja[i].kab%></td>
                                <td><%= data_tenaga_kerja[i].ska%></td>
                                <td><%= data_tenaga_kerja[i].skt%></td>
                            </tr>
                            <%}%>

                            <!-- <tr>
                                <td><a href="#"> Kabupaten Banyumas</a></td>
                                <td>200</td>
                            </tr>

                            <tr>
