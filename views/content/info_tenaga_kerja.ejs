                </div>
            </div>
        </div>
    </section> -->

    <% include ../template/footer.ejs %>


    <script src="/javascripts/template/jquery.min.js"></script>
    <script src="/javascripts/template/bootstrap.bundle.min.js"></script>
    <!-- Plugins This Page -->
    <script src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.min.js"></script>
       <script src="https://cdn.datatables.net/1.10.22/js/dataTables.bootstrap4.min.js"></script>
       <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
    <!-- End Plugins This Page -->
    <script type="text/javascript">
    $(function() {
      $('#menu-tenaga-kerja').addClass('custom');
    });
      $('.datane').DataTable({
        "pageLength": 35,
        "searching": false
      });
      var data_ska=[];
    var data_skt=[];
    <% for(var i=0; i<data_ska.length; i++){ %>
        data_ska.push( { label: "<%= data_ska[i].kab%>", y: <%= data_ska[i].ska%> });
    <%}%>
    <% for(var i=0; i<data_skt.length; i++){ %>
        data_skt.push( { label: "<%= data_skt[i].kab%>", y: <%= data_skt[i].skt%> });
    <%}%>
      var chart = new CanvasJS.Chart("chartContainer", {
        animationEnabled: true,
        toolTip: {
		shared: true
	},

    legend:{
  fontSize: 14,
 },
        axisX: {
            interval:1,
            labelFontSize: 14,
	    },

        axisY: {
            // interval:1,
            labelFontSize: 14,
	    },
        data: [{
            type: "bar",
            name: "Tenaga Kerja SKA",
            showInLegend: true,
            dataPoints: data_ska
        },{
            type: "bar",
            showInLegend: true,
            name: "Tenaga Kerja SKT",
            dataPoints: data_skt
        }]
    });
    chart.render();


    var chart2 = new CanvasJS.Chart("chartContainer2", {
        animationEnabled: true,
        axisX: {
            interval:1,
	    },
        data: [{
            type: "column",
            name: "Proven Oil Reserves (bn)",
            dataPoints:[
                { label: "Arsitektur", y: 266.21 },
                { label: "Sipil", y: 302.25 },
                { label: "Mekanikal", y: 157.20 },
                { label: "Elektrikal", y: 148.77 },
                { label: "Tata Lingkungan", y: 101.50 },
            ]
        }]
    });
    chart2.render();
    

    $('.btn-detail').click(function() {
            $('html, body').animate({
            scrollTop: $("#detail").offset().top
            }, 1000);
    });
    </script>
</body>
