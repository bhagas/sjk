<% include ../../template-backoffice/header.ejs %>
<!-- BEGIN PAGE CONTENT -->
<div class="page-content">
  <div class="header">
    <h2><strong>Tambah Data Susunan TPJK</strong></h2>
    <div class="breadcrumb-wrapper">
      <ol class="breadcrumb">
        <li><a href="/backoffice">Dashboard</a></li>
        <li><a href="/manajemen_pembina">Data</a></li>
        <li class="active">Tambah Data</li>
      </ol>
    </div>
  </div>

  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
      <div class="panel">
        <div class="panel-content">
          <form role="form" action="/manajemen_pembina/susunan_tpjk/submit_insert" method="POST" enctype="multipart/form-data">
            <input type="hidden" class="form-control" name="id_pembina" id="" value="<%= data[0].id%>">

            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                <div class="form-group">
                  <label>Nama / Jabatan Kedinasan</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="jabatan_kedinasan" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Nama</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="nama" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Jabatan</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="jabatan" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Email</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="email" id="">
                    </div>
                  </div>
                </div>
              </div>

              <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                

                <div class="form-group">
                  <label>Alamat Kantor</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="alamat" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Telepon Kantor</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="telepon" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Fax</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                      <input type="text" class="form-control" name="fax" id="">
                    </div>
                  </div>
                </div>
              </div>
            </div>

            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <button type="submit" class="btn btn-primary btn-embossed">Simpan</button>
              </div>
            </div>
          </form>

          <div class="row">
            <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
              <table class="table table-bordered">
                <thead>
                  <tr>
                    <th>Nama / Jabatan Kedinasan</th>
                    <th>Nama</th>
                    <th>Jabatan</th>
                    <th>Email</th>
                    <th>Alamat Kantor</th>
                    <th>Telepon Kantor</th>
                    <th>Fax</th>
                    <th>Option</th>
                  </tr>
                </thead>

                <tbody>
                  <% for(var i=0; i<tpjk.length; i++){ %>
                  <tr>
                    <td><%= tpjk[i].jabatan_kedinasan%></td>
                    <td><%= tpjk[i].nama%></td>
                    <td><%= tpjk[i].jabatan%></td>
                    <td><%= tpjk[i].email%></td>
                    <td><%= tpjk[i].alamat%></td>
                    <td><%= tpjk[i].telepon%></td>
                    <td><%= tpjk[i].fax%></td>
                    <td><a href="/manajemen_pembina/susunan_tpjk/delete/<%= tpjk[i].id%>/<%= tpjk[i].id_pembina%>" class="btn btn-danger btn-sm btn-embossed" onclick="return confirm('Apakah anda yakin untuk menghapus data ini ?');">Hapus</a></td>
                  </tr>
                  <%}%>
                </tbody>
              </table>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="footer">
    <div class="copyright">
      <span>Copyright <span class="copyright">Â©</span> 2021 </span>
        <span>Dinas PU Bina Marga dan Cipta Karya Provinsi Jawa Tengah</span>.
  </div>
</div>
<!-- END PAGE CONTENT -->

<% include ../../template-backoffice/footer.ejs %>
