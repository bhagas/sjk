<% include ../../template-backoffice/header.ejs %>
<!-- BEGIN PAGE CONTENT -->
<div class="page-content">
  <div class="header">
    <h2><strong>Tambah Data Badan Usaha Jasa Pelaksana Konstruksi</strong></h2>
    <div class="breadcrumb-wrapper">
      <ol class="breadcrumb">
        <li><a href="/backoffice">Dashboard</a></li>
        <li><a href="/manajemen_badan_usaha">Data</a></li>
        <li class="active">Tambah Data</li>
      </ol>
    </div>
  </div>

  <div class="row">
    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
      <div class="panel">
        <div class="panel-content">
          <form role="form" action="/manajemen_badan_usaha/jasa_pelaksana/submit_insert" method="POST">
            <div class="row">
              <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                <div class="form-group">
                  <label>Kabupaten/Kota</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                      <select class="form-control" name="id_kab" id="id_kab" data-search="true">
                        <option value="-">-- Pilih --</option>
                       
                        <% for(var i=0; i<kabupaten.length; i++){ %> 
                          
                          <option value="<%= kabupaten[i]['id_kab']%>"><%= kabupaten[i]['kab']%></option>
                          <% }%>
                      </select>
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Jumlah Badan Usaha Klasifikasi Bangunan Gedung</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                      <input type="text" class="form-control" name="ska" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Jumlah Badan Usaha Klasifikasi Bangunan Sipil</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                      <input type="text" class="form-control" name="skt" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Jumlah Badan Usaha Klasifikasi Mekanikal Elektrikal</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                      <input type="text" class="form-control" name="skt" id="">
                    </div>
                  </div>
                </div>

                <div class="form-group">
                  <label>Jumlah Badan Usaha Klasifikasi Jasa Pelaksana Lainnya</label>
                  <div class="row">
                    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                      <input type="text" class="form-control" name="skt" id="">
                    </div>
                  </div>
                </div>

                <button type="submit" class="btn btn-primary btn-embossed">Simpan</button>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </div>
  <div class="footer">
    <div class="copyright">
      <p class="pull-left sm-pull-reset">
        <span>Copyright <span class="copyright">Â©</span> 2018 </span>
        <span>Pemerintah</span>.
      </p>
    </div>
  </div>
</div>
<!-- END PAGE CONTENT -->

